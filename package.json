{
  "name": "nextjs",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next  -p 8000",
    "build": "next build",
    "start-local": "cross-env FORMAL=true next start -p 8000",
    "start": "cross-env FORMAL=true next start -p 8000",
    "start-t1": "next start -p 8000",
    "publish-t1": "next build && node cdn.js",
    "publish": "cross-env FORMAL=true next build && node cdn.js",
    "pm2-t1": "pm2 start npm --no-pmx --name next -- run start-t1",
    "pm2": "pm2 start npm --no-pmx --name next -- start",
    "type-check": "tsc",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint:js": "eslint . --fix",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.css\"",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "postinstall": "husky || true"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint:js",
    "**/*.{js,jsx,tsx,ts,css,scss,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ai-sdk/react": "^1.1.17",
    "@ant-design/cssinjs": "^1.23.0",
    "@ant-design/icons": "^5.5.1",
    "@reduxjs/toolkit": "^2.3.0",
    "ahooks": "^3.8.4",
    "animate.css": "^4.1.1",
    "antd": "^5.24.1",
    "antd-img-crop": "^4.23.0",
    "antd-mobile": "^5.39.0",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "cookie": "^1.0.2",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "js-cookie": "^3.0.5",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "mime": "^4.0.4",
    "nanoid": "^5.0.9",
    "next": "^15.3.0",
    "next-redux-wrapper": "^8.1.0",
    "next-seo": "^6.6.0",
    "rc-virtual-list": "^3.18.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-easy-crop": "^5.4.1",
    "react-lazyload": "^3.2.1",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "sass": "^1.80.3"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.679.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.6.0",
    "@next/bundle-analyzer": "^12.0.7",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.0",
    "@types/crypto-js": "^4.2.2",
    "@types/eslint__js": "^8.42.3",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "22.9.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-lazyload": "^3.2.3",
    "autoprefixer": "^10.4.19",
    "commitlint": "^19.3.0",
    "eslint": "^9.6.0",
    "eslint-config-next": "^15.1.6",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "p-limit": "^6.1.0",
    "ping": "^0.4.2",
    "postcss": "^8.4.39",
    "postcss-pxtorem": "^6.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.4.2",
    "rd": "^2.0.1",
    "react-flip-move": "^3.0.4",
    "stylelint": "^16.6.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^4.1.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.11.0"
  }
}